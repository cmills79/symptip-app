# Example environment variables for local development
# Do NOT store secrets in the repo. Copy this to .env and fill values locally.

# Next.js
NEXT_PUBLIC_APP_NAME=Symptiq

# Firebase (use the emulator or project-specific values)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com

# Firebase Admin (server-side functions)
FIREBASE_ADMIN_CLIENT_EMAIL=service-account@example.iam.gserviceaccount.com
FIREBASE_ADMIN_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"

# Anthropic / Claude (for AI features) - set in CI or local env securely
ANTHROPIC_API_KEY=sk-xxxx

# Databricks (optional)
DATABRICKS_HOST=https://adb-1234567890.0.azuredatabricks.net
DATABRICKS_TOKEN=dapiXXXXXXXXXXXXXXXX

# Other (examples)
NODE_ENV=development
PORT=3000
# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Anthropic Claude API
ANTHROPIC_API_KEY=

# Nutritionix API (via Databricks)
NUTRITIONIX_APP_ID=
NUTRITIONIX_API_KEY=

# Databricks Configuration
DATABRICKS_HOST=
DATABRICKS_TOKEN=

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
